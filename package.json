{
	"name": "api",
	"version": "1.0.0",
	"description": "",
	"private": true,
	"scripts": {
		"db:stop": "docker-compose stop db",
		"db:start": "docker-compose start db",
		"db:remove": "docker-compose down",
		"db:create": "docker-compose up -d",
		"db:setup": "node dist/db/setup.js",
		"setup": "npm run build && npm run db:remove && npm run db:create && npm run db:setup",
		"build:server": "tsc --build tsconfig.json",
		"build:client": "npx webpack --config webpack.config.js",
		"watch:server": "tsc --build tsconfig.json --watch",
		"watch:client": "npx webpack --config \"webpack.config.js\" --watch",
		"clean": "rm -rf dist",
		"build": "npm run build:server && npm run build:client",
		"start:local": "npm run db:start && node dist/api/index.js",
		"start": "node dist/api/index.js",
		"prettier": "prettier --write \"./**/*.{j{s,sx},t{s,sx},json}\""
	},
	"author": "",
	"license": "ISC",
